{
    "manifest_version": 3,
    "name": "AADL3 Auto Register Extension",
    "description": "AADL 3 Auto: Refresh and Input filler",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "res/icon32.png"
    },
    "author": {
      "name":  "ABES Mounir",
      "email": "mounir2013b@gmail.com"
    },

    "background": {
      "service_worker": "js/service_worker.js"
    },

    "icons": {
      "16":  "res/icon32.png",
      "32":  "res/icon64-2.png",
      "48":  "res/icon64-2.png",
      "128": "res/icon128-2.png"
    },

    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "scripting",
      "webRequest",
      "notifications",
      "webNavigation"
    ],

    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ]

  }